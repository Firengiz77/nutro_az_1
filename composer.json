{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "benjaminhirsch/nova-slug-field": "^1.3",
        "beyondcode/nova-custom-dashboard-card": "^0.0.1",
        "codenco-dev/nova-grid-system": "^1.0",
        "daniel-de-wit/nova-single-record-resource": "^1.2",
        "digital-creative/nova-dashboard": "^0.0.1",
        "digital-creative/nova-json-wrapper": "^0.2.0",
        "digitalcloud/multilingual-nova": "^2.0",
        "drobee/nova-sluggable": "^1.2",
        "ebess/advanced-nova-media-library": "^3.4",
        "emilianotisato/nova-tinymce": "^1.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "joedixon/nova-translation": "^1.0",
        "kongulov/nova-tab-translatable": "^1.0",
        "laravel/framework": "^8.40",
        "laravel/nova": "~3.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.3",
        "optimistdigital/nova-multiselect-field": "^2.3",
        "optimistdigital/nova-sortable": "^2.3",
        "orlyapps/nova-belongsto-depend": "^3.0",
        "spatie/laravel-medialibrary": "^9.6",
        "yoeunes/toastr": "^1.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
